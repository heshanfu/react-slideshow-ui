{
  "name": "react-slideshow-ui",
  "version": "1.0.4",
  "author": "Masashi Hirano (https://github.com/shisama)",
  "license": "MIT",
  "description": "React UI Component for slideshow like SlideShare or SpeakerDeck.",
  "main": "lib/SlideShow.js",
  "homepage": "https://github.com/shisama/react-slideshow-ui",
  "bugs": {
    "url": "https://github.com/shisama/react-slideshow-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shisama/react-slideshow-ui.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "slideshow",
    "slide",
    "slides",
    "slideshare",
    "speakerdeck",
    "presentation",
    "keynote",
    "powerpoint"
  ],
  "scripts": {
    "build": "npm run clean && babel --no-comments src --out-dir lib --ignore src/demo",
    "build:demo": "NODE_ENV=development webpack",
    "build:dll": "NODE_ENV=production webpack --config webpack.dll.config.js",
    "build:all": "npm run lint && npm run typecheck && npm test && npm run build:dll && npm run build:demo && npm run clean && npm run build",
    "clean": "rimraf lib",
    "test": "NODE_ENV=test jest --verbose --coverage",
    "lint": "eslint src --ext js,jsx --fix",
    "typecheck": "flow",
    "remove-types": "flow-remove-types --out-dir out/ src/ && eslint out --ext js,jsx --fix",
    "prepublishOnly": "npm test && npm build",
    "git-add": "git add lib demo dll",
    "typed": "flow-typed install --skip",
    "convert-ts": "NODE_ENV=ts babel src/SlideShow.jsx -o src/SlideShow.tsx"
  },
  "dependencies": {
    "@babel/preset-flow": "7.0.0-beta.44",
    "react": "^16.3.2",
    "recompose": "^0.26.0",
    "toggle-fullscreen": "^0.3.1"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.44",
    "@babel/core": "7.0.0-beta.44",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.44",
    "@babel/plugin-syntax-flow": "7.0.0-beta.44",
    "@babel/plugin-transform-flow-strip-types": "7.0.0-beta.44",
    "@babel/plugin-transform-object-assign": "7.0.0-beta.44",
    "@babel/polyfill": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@babel/preset-flow": "7.0.0-beta.44",
    "@babel/preset-react": "7.0.0-beta.44",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-flow-to-typescript": "^0.2.0",
    "browser-env": "^3.2.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-closure": "^0.0.1",
    "eslint-config-google": "^0.9.1",
    "eslint-config-prettier": "^2.2.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.59.0",
    "flow-remove-types": "^1.2.1",
    "flow-typed": "^2.4.0",
    "jest": "^22.4.3",
    "prettier": "^1.12.1",
    "prop-types": "^15.6.1",
    "react-dom": "^16.3.2",
    "react-hot-loader": "1.3.1",
    "react-test-renderer": "^16.3.2",
    "rimraf": "^2.6.1",
    "sinon": "^4.5.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "browsers": [
                  "last 2 versions"
                ]
              },
              "useBuiltIns": true
            }
          ],
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-transform-object-assign"
        ]
      },
      "production": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "browsers": [
                  "last 2 versions",
                  "IE 11"
                ]
              },
              "useBuiltIns": true
            }
          ],
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-transform-object-assign"
        ]
      },
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "current"
              }
            }
          ],
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-transform-object-assign"
        ]
      },
      "ts": {
        "presets": [
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-proposal-class-properties",
          "babel-plugin-flow-to-typescript"
        ]
      }
    }
  }
}
